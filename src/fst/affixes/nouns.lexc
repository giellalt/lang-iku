! =============== !
! Noun morphology !
! =============== !

! Stem types
! ==========

LEXICON vst
+N+Sg:     SGCASE ;
+N+Sg:     SGPOSS ;
+N+Du:%>%^Vk DUCASE ; ! du-k
+N+Du:%>%^Vk DUPOSS ; ! du-k
+N+Pl:     PLCASE ;
+N+Pl:     PLPOSS ;


LEXICON kst
+N+Sg:            SGCASE ;
+N+Sg:            SGPOSS ;
+N+Du:%^TRUNC%>%^Vk DUCASE ;
+N+Du:%^TRUNC%>     DUPOSS ; !or %^Vk?
+N+Pl:            PLCASE ;
+N+Pl:            PLPOSS ;

LEXICON qst
+N+Sg: SGCASE ;
+N+Sg: SGPOSS ;
+N+Du:%>k DUCASE ;
+N+Du:%>k DUPOSS ;
+N+Pl: PLCASE ;
+N+Pl: PLPOSS ;


! New strategy:

! stem
! sg/du/pl
! -> basic decl x8
! -> possecl x 6/9 x 8



! Absolute declension
! =================

! Sg
! --

LEXICON SGCASE
+Abs:  clit ;
+Gen:%>up  clit ;
+Acc:%>mik  clit ;
+Loc:%>mi  clit ;
+All:%>mut  clit ;
+Abl:%>mit  clit ;
+Tra:%>kkut  clit ;
+Sim:%>titut  clit ;


! Du
! --

LEXICON DUCASE
+Abs:  clit ;
+Gen:  clit ;
+Acc:%>nik  clit ;
+Loc:%>ni  clit ;
+All:%>nut  clit ;
+Abl:%>nit  clit ;
+Tra:%>kut  clit ;
+Sim:%>titut  clit ;


! Pl
! --

LEXICON PLCASE
+Abs:%>it   clit ;
+Gen:%>it	 clit ;
+Acc:%>nik	 clit ;
+Loc:%>ni	 clit ;
+All:%>nut	 clit ;
+Abl:%>nit	 clit ;
+Tra:%>tigut clit ;
+Sim:%>titut clit ;


! Possessor paradigms
! ===================

LEXICON SGPOSS
SGPOSS1SG ;
SGPOSS2SG ;
SGPOSS3SG ;
SGPOSS1DU ;
SGPOSS2DU ;
SGPOSS3DU ;
SGPOSS1PL ;
SGPOSS2PL ;
SGPOSS3PL ;

LEXICON SGPOSS1SG
+1SgPoss+Abs:%>ga clit ;
+1SgPoss+Gen:%>ma clit ;
+1SgPoss:%>ga     ADVLPOSSCASE ;

LEXICON SGPOSS2SG
+2SgPoss: clit ;

LEXICON SGPOSS3SG
+3SgPoss: clit ;

LEXICON SGPOSS1DU
+1DuPoss: clit ;

LEXICON SGPOSS2DU
+2DuPoss: clit ;

LEXICON SGPOSS3DU
+3DuPoss: clit ;

LEXICON SGPOSS1PL
+1PlPoss: clit ;

LEXICON SGPOSS2PL
+2PlPoss: clit ;

LEXICON SGPOSS3PL
+3PlPoss: clit ;


LEXICON DUPOSS
DUPOSS1SG ;
DUPOSS2SG ;
DUPOSS3SG ;
DUPOSS1DU ;
DUPOSS2DU ;
DUPOSS3DU ;
DUPOSS1PL ;
DUPOSS2PL ;
DUPOSS3PL ;

LEXICON DUPOSS1SG
+1SgPoss+Abs:%>kka clit ;
+1SgPoss+Gen:%>mma clit ;
+1SgPoss:%>kka     ADVLPOSSCASE ;

LEXICON DUPOSS2SG
# ;
LEXICON DUPOSS3SG
# ;
