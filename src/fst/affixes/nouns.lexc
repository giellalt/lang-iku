! The adverbial cases


LEXICON ADVLPOSSCASE
+Acc:%>nik  clit ;
+Loc:%>ni   clit ;
+All:%>nut  clit ;
+Abl:%>nit  clit ;
+Tra:%>gut  clit ;
+Sim:%>tut  clit ;

























